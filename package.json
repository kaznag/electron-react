{
  "name": "electron-react",
  "version": "0.1.0",
  "main": "main.js",
  "author": "Kaz NAGAMINE",
  "license": "MIT",
  "scripts": {
    "start": "electron ./dist/prod",
    "build": "node ./scripts/package.js && webpack --config ./config/webpack.config.prod.js",
    "clean": "rimraf ./dist/prod",
    "rebuild": "npm run clean && npm run build",
    "start:dev": "electron ./dist/dev",
    "build:dev": "node ./scripts/package.js && webpack --config ./config/webpack.config.dev.js",
    "clean:dev": "rimraf ./dist/dev",
    "rebuild:dev": "npm run clean:dev && npm run build:dev",
    "pack:l": "npm run rebuild && electron-builder --l --config ./config/electron-builder.yml",
    "pack:w": "npm run rebuild && electron-builder --w --config ./config/electron-builder.yml",
    "pretest": "npm run lint",
    "test": "jest --config ./config/jest.config.js",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check --config ./config/.prettierrc.yml config/**/*.{js,json,yml}  src/**/*",
    "lint:stylelint": "stylelint --config ./config/.stylelintrc.yml src/**/*.{css,sass,scss}",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write --config ./config/.prettierrc.yml config/**/*.{js,json,yml} src/**/*",
    "fix:stylelint": "stylelint --config ./config/.stylelintrc.yml --fix src/**/*.{css,sass,scss}"
  },
  "keywords": [
    "electron",
    "react",
    "webpack",
    "electron-builder",
    "typescript",
    "localization",
    "i18next",
    "redux",
    "jest"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "electron-store": "^8.0.0",
    "i18next": "^20.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.1",
    "react-redux": "^7.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/i18next": "^13.0.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-i18next": "^8.1.0",
    "@types/react-redux": "^7.1.18",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "electron": "28.3.1",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "javascript-obfuscator": "^2.15.5",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.75.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "typescript-transformer-jsx-remove-data-test-id": "^1.1.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.3.2"
  },
  "volta": {
    "node": "18.20.2",
    "yarn": "1.22.22"
  }
}
